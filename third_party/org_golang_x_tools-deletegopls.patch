-	if c.Params != nil {
-		// we're receiving it, so it must be ToClient
-		return parse.ToClient
-	}
-	if c.Result == nil {
-		if c.ID != nil {
-			return parse.ClResponse
-		}
-		log.Printf("%+v", *c)
