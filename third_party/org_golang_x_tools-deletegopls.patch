-			return false
-		}
-		fi, err := fd.Stat()
-		if err != nil {
-			return false
-		}
-		return fi.Mode()&0111 != 0
-	}
-	for _, t := range totry {
-		g := os.Getenv(t[0])
-		if g != "" && ok(g+t[1]) {
-			gopls := g + t[1]
